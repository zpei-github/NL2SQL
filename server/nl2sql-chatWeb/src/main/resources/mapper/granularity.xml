<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zpei.nl2sql.web.mapper.mysql.standard_database.GranularityMapper">
    <select id="getAllGranularity" resultType="com.zpei.nl2sql.web.entity.mysql.standard_database.Granularity">
