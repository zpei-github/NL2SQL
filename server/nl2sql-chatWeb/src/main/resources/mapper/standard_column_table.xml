<mapper namespace="com.zpei.nl2sql.web.mapper.mysql.standard_database.StandardColumnTableMapper">
    <select id="getAllStandardColumnTable" resultType="com.zpei.nl2sql.web.entity.mysql.standard_database.StandardColumnTable">
