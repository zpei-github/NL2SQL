<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.standard_database.StandardColumnTableMapper">
    <select id="getAllStandardColumnTable" resultType="com.web.entity.StandardColumnTable">
        SELECT
            *
        FROM
            standard_column_table
        WHERE
            standard_table_name IS NOT NULL
            AND standard_column_name IS NOT NULL
    </select>
</mapper>