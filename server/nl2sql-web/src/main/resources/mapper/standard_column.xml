<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.standard_database.StandardColumnMapper">
    <select id="getAllStandardColumn" resultType="com.web.entity.StandardColumn">
        SELECT
            *
        FROM
            standard_column
        WHERE
            standard_column_name IS NOT NULL
    </select>
</mapper>