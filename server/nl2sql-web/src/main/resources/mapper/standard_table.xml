<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.standard_database.StandardTableMapper">
    <select id="getAllStandardTables" resultType="com.web.entity.StandardTable">
        SELECT
            *
        FROM
            standard_table
        WHERE
            standard_table_name IS NOT NULL
    </select>
</mapper>